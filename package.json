{
  "name": "quejas-entidades-boy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:reset": "npm run db:destroy && npm run db:setup",
    "db:destroy": "docker-compose down -v",
    "db:setup": "docker-compose up -d && node -e \"setTimeout(() => {}, 5000)\" && npm run db:migrate && npm run db:seed",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "pug": "^2.0.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.37.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "babel-jest": "^30.0.5",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "supertest": "^7.1.4"
  }
}
