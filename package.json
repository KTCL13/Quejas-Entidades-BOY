{
  "name": "quejas-entidades-boy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:reset": "npm run db:destroy && npm run db:setup",
    "db:destroy": "docker-compose down -v",
    "db:setup": "docker-compose up -d && node -e \"setTimeout(() => {}, 5000)\" && npm run db:migrate && npm run db:seed",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.7",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.35.0",
    "babel-jest": "^30.0.5",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
